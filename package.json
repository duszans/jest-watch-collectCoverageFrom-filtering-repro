{
  "name": "jest-repro",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test:watch:withCoverageFrom": "jest --watch --coverage",
    "test:watch:findRelatedTests": "jest --watch --coverage --findRelatedTests"
  },
  "jest": {
    "testRegex": ".*-test\\.js$",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**"
    ]
  },
  "dependencies": {
    "jest": "^23.6.0"
  }
}
